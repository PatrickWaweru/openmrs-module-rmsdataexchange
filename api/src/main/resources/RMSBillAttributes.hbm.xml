<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.rmsdataexchange.queue.model">


    <class name="org.openmrs.module.rmsdataexchange.queue.model.BillAttribute" table="cashier_bill_attribute">
		<cache usage="read-write" />

		<id name="id" type="int" column="bill_attribute_id">
			<generator class="native">
				<param name="sequence">cashier_bill_attribute_id_seq</param>
			</generator>
		</id>
		<discriminator column="bill_attribute_id" insert="false" />

		<many-to-one name="owner" class="org.openmrs.module.kenyaemr.cashier.api.model.Bill" column="bill_id" not-null="true" />
		<many-to-one name="attributeType" class="org.openmrs.module.rmsdataexchange.queue.model.BillAttributeType" column="bill_attribute_type_id" not-null="true" />
		<property name="value" type="text" access="field" column="value" length="65535" />

		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
	</class>

    <class name="org.openmrs.module.rmsdataexchange.queue.model.BillAttributeType" table="cashier_bill_attribute_type">
		<cache usage="read-write" />

		<id name="id" type="int" column="bill_attribute_type_id">
			<generator class="native">
				<param name="sequence">cashier_bill_attribute_type_id_seq</param>
			</generator>
		</id>
		<discriminator column="bill_attribute_type_id" insert="false" />

		<many-to-one name="owner" class="org.openmrs.module.kenyaemr.cashier.api.model.Bill" column="bill_id" not-null="true" />
		<many-to-one name="attributeType" class="org.openmrs.module.rmsdataexchange.queue.model.BillAttributeType" column="bill_attribute_type_id" not-null="true" />
		<property name="value" type="text" access="field" column="value" length="65535" />

		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
	</class>

</hibernate-mapping>