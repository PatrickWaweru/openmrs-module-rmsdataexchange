        <!-- Constraints -->

        <addUniqueConstraint 
            columnNames="uuid" 
            constraintName="uk_cashier_bill_attribute_uuid" 
            tableName="cashier_bill_attribute"/>

        <addForeignKeyConstraint 
            constraintName="fk_bill_attr_changer"
            baseTableName="cashier_bill_attribute"
            baseColumnNames="changed_by"
            referencedTableName="users"
            referencedColumnNames="user_id"/>

        <addForeignKeyConstraint 
            constraintName="fk_bill_attr_creator"
            baseTableName="cashier_bill_attribute"
            baseColumnNames="creator"
            referencedTableName="users"
            referencedColumnNames="user_id"/>

        <addForeignKeyConstraint 
            constraintName="fk_bill_attr_voider"
            baseTableName="cashier_bill_attribute"
            baseColumnNames="voided_by"
            referencedTableName="users"
            referencedColumnNames="user_id"/>

        <addForeignKeyConstraint 
            constraintName="fk_bill_attr_type"
            baseTableName="cashier_bill_attribute"
            baseColumnNames="bill_attribute_type_id"
            referencedTableName="cashier_bill_attribute_type"
            referencedColumnNames="bill_attribute_type_id"/>

        <addForeignKeyConstraint 
            constraintName="fk_identifies_bill"
            baseTableName="cashier_bill_attribute"
            baseColumnNames="bill_id"
            referencedTableName="cashier_bill"
            referencedColumnNames="bill_id"/>

        <!-- Indexes -->
        <createIndex indexName="attribute_changer" tableName="cashier_bill_attribute">
            <column name="changed_by"/>
        </createIndex>

        <createIndex indexName="attribute_creator" tableName="cashier_bill_attribute">
            <column name="creator"/>
        </createIndex>

        <createIndex indexName="defines_attribute_type" tableName="cashier_bill_attribute">
            <column name="bill_attribute_type_id"/>
        </createIndex>

        <createIndex indexName="identifies_bill" tableName="cashier_bill_attribute">
            <column name="bill_id"/>
        </createIndex>

        <createIndex indexName="attribute_voider" tableName="cashier_bill_attribute">
            <column name="voided_by"/>
        </createIndex>
    </changeSet>

